// swiftlint:disable all

import Foundation

#if canImport(ChangeableCopy)
import ChangeableCopy
#endif

{% for type in types.structs where type.based.AutoChangeable %}
{% if type.accessLevel != "private" %}
extension {{ type.name }} {

    {{ type.accessLevel }} init(from copy: ChangeableCopy<{{ type.name }}>) {
        self.init(
        {% for variable in type.storedVariables %}
            {% if forloop.last %}
            {{ variable.name }}: copy.{{ variable.name }}
            {% else %}
            {{ variable.name }}: copy.{{ variable.name }},
            {% endif %}
        {% endfor %}
        )
    }
}

{% endif %}
{% endfor %}
